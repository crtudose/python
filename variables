greeting = "Hello, friend!"
print(greeting)
age = 35 # integer
speed = 4.5 # float
am_i_ugly = False # bool
print(age)
print(speed)
print(am_i_ugly)
var = '23' #string
print(var) 
print(0.0000000000004) #4e-13
print(0o123) #83 = 64+16+3 octal
print(0x123) #291= 256+32+3 hexadecimal
print(2+3) #addition
print(5-4) #substract
print(5*3) #multiplication
print(7/2) #division
print(7//2) #integer division
print(7%2) #modulo
print(6**3) #power
print(2+3.0)
